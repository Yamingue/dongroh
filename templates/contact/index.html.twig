{% extends 'base.html.twig' %}

{% block title %}Hello ContactController!{% endblock %}

{% block body %}
<!-- Material form register -->
<div class="card mt-2">

    <h5 class="card-header indigo white-text py-4">
        <strong> {% trans %} Bienvenue chez DONGROH. Ce formulaire de contact Vous est reserver pour toute preocupation qu'il
            s'agis d'une remarque ou d'une demande de service customis√© votre plaisir est la notre {% endtrans %}</strong>
    </h5>

    <!--Card content-->
    <div class="card-body ">
        
        {{ form_start(form)}}
        {{ form_widget(form)}}
        <button type="submit" href="#" class="btn btn-orange btn-icon-split">
            <span class="icon text-white-50">
                <i class="fas fa-paper-plane"></i>
            </span>
            <span class="text">Envoyer</span>
        </button>
        {{ form_end(form)}}
    </div>

</div>
<!-- Material form register -->

{% endblock %}